 {{#each posts as |post|}}
        <div class="poststop">
            <div class="postTitle"><a href="/post/{{post_id}}">{{post_title}}</a></div>
            <div class="postMeta">Created by {{User.user_name}} on {{post.date_created}}</div>
        </div>
        <div class="postContent"> {{post.content}}</div>
    {{/each}}